<https://padovagrandtour.github.io/entitites#https://padovamusei.it/sites/default/files/styles/immagine_opera/public/opera/copertina/2971.jpg?itok=fiSI07AF> a pgt:Artwork ;
    pgt:conservationState "Buono"^^xsd:string ;
    pgt:height "270.0"^^xsd:float ;
