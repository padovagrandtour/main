<https://padovagrandtour.github.io/entitites#https://padovamusei.it/sites/default/files/styles/immagine_opera/public/opera/copertina/9334.jpg?itok=q9KePyDL> a pgt:Artwork ;
    pgt:conservationState "Buono"^^xsd:string ;
    pgt:height "92.0"^^xsd:float ;
