pgt:ARTWORK630 a pgt:Artwork ;
    pgt:conservationState "Buono"^^xsd:string ;
    pgt:dimensionsText "Altezza: 92  cm Larghezza: 143  cm"^^xsd:string ;
    pgt:hasCollection pgt:COLLECTION22 ;
