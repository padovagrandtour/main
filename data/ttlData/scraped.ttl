pgt:ARTWORK692 a pgt:Artwork ;
    pgt:conservationState "Buono"^^xsd:string ;
    pgt:dimensionsText "Altezza: 40  cm Larghezza: 54  cm"^^xsd:string ;
    pgt:hasCollection pgt:COLLECTION22 ;
