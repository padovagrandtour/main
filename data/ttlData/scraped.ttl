pgt:ARTWORK32 a pgt:Artwork ;
    pgt:conservationState "Buono"^^xsd:string ;
    pgt:dimensionsText "Altezza: 200 cm ca.Larghezza: 200 cm ca."^^xsd:string ;
    pgt:hasCollection pgt:COLLECTION1 ;
