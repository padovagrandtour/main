pgt:ARTWORK350 a pgt:Artwork ;
    pgt:conservationState "Buono"^^xsd:string ;
    pgt:depth "73.0"^^xsd:float ;
    pgt:dimensionsText "Altezza: 92 cm Larghezza: 145 cm Profondità: 73 cm"^^xsd:string ;
