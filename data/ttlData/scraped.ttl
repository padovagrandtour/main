pgt:ARTWORK665 a pgt:Artwork ;
    pgt:conservationState "Buono"^^xsd:string ;
    pgt:dimensionsText "Altezza: 330  cm Larghezza: 390  cm"^^xsd:string ;
    pgt:hasCollection pgt:COLLECTION22 ;
