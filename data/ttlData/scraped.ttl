pgt:ARTWORK21 a pgt:Artwork ;
    pgt:conservationState "Buono"^^xsd:string ;
    pgt:dimensionsText "Altezza: 203 cm ca.Larghezza: 217 cm ca."^^xsd:string ;
    pgt:hasCollection pgt:COLLECTION1 ;
