    sdo:name "Angelo che tiene tra le mani un'animula (Angelo)"^^xsd:string ;
    sdo:url "https://padovamusei.it/it/musei/museo-arte-medievale-moderna/collezioni/dipinti/angelo-che-tiene-tra-mani-%27animula-angelo"^^sdo:URL .

