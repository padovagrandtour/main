    sdo:name "Paesaggio con arco naturale su un corso d'acqua e figure"^^xsd:string ;
    sdo:url "https://padovamusei.it/it/musei/museo-arte-medievale-moderna/collezioni/dipinti/paesaggio-arco-naturale-corso-acqua-figure"^^sdo:URL .

